{
    "name": "jhonzya/supercell-api-php",
    "description": "The simplest PHP wrapper for Supercell API",
    "type": "library",
    "homepage": "https://github.com/jhonzya/supercell-api-php",
    "license": "GNU Public License",
    "authors": [
        {
            "name": "Jonathan Salinas",
            "email": "a.jonathan.sl@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0|^7.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "keywords": [
        "supercell",
        "clashofclans",
        "clashroyale",
        "brawlstars",
        "api",
        "php"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Jhonzya\\SupercellApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jhonzya\\SupercellApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
