{
    "name": "jhonzya/supercell-api-php",
    "description": "The simplest PHP wrapper for Supercell API",
    "keywords": [
        "supercell",
        "clashofclans",
        "clashroyale",
        "brawlstars",
        "api",
        "php"
    ],
    "homepage": "https://github.com/jhonzya/supercell-api-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jonathan Salinas",
            "email": "a.jonathan.sl@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^7.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Jhonzya\\SupercellApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jhonzya\\SupercellApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
